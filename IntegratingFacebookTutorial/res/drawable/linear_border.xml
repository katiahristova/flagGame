<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="10dp" android:height="10dp"
                android:color="#f56954" />
            <solid android:color="#99b5b5" />
            <padding android:left="1dp" android:top="1dp" android:right="1dp"
                android:bottom="1dp" />

            <corners android:radius="1dp" android:bottomRightRadius="1dp"
                android:bottomLeftRadius="1dp" android:topLeftRadius="1dp"
                android:topRightRadius="1dp" />
        </shape>
    </item>

</layer-list>